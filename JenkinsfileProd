pipeline {
    
    agent {
        label 'CICD'
    } 
    
    environment {
        IMAGE_TAG = "${BUILD_NUMBER}"
    }
    
    stages {
        
        stage('Checkout'){
           steps {
                git credentialsId: 'washbingit', 
                url: 'https://github.com/shettymanish01/WashBin',
                branch: 'dev'
                
           }
        }

        

        
        stage('Deploy to k8s'){
            steps{
                script{
                    kubernetesDeploy (configs: 'deploymentservice.yaml',kubeconfigId: 'k8sconfigpwd')
                }
            }
        }
       
    }
}
